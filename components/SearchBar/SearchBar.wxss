/* components/SearchBar/SearchBar.wxss */
.search_bar {
  height: 90rpx;
  padding: 10rpx;
  background-color: var(--themeColor);
}
.search_bar navigator {
  height: 100%;
  display: flex;
  /*伸缩盒子*/
  background-color: white;
  justify-content: center;
  align-items: center;
  color: #666;
  border-radius: 15rpx;
}
.search_bar navigator icon {
  padding-right: 5rpx;
}
